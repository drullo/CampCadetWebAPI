  <connectionStrings>
    <add name="CampCadet" connectionString="Data Source=tcp:184.168.194.77; Initial Catalog=CampCadet; Persist Security Info=True; User ID=campcadet; Password=c@mpCad3t;" providerName="System.Data.SqlClient"/>
